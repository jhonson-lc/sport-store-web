---
import LinkItem from '../LinkItem.astro';
import Logo from '../Logo.astro';

export interface Props {
  links: Array<{
    href: string;
    title: string;
  }>;
}

const { links } = Astro.props;
---

<nav class="flex items-center bg-black justify-center gap-36 py-[50px]">
  <Logo />
  <ul class="flex gap-12">
    {links.map((link) => <LinkItem href={link.href} title={link.title} />)}
  </ul>
</nav>


